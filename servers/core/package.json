{
  "name": "@servers/core",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "npx tsc",
    "dev": "npm run build && concurrently \"npx tsc --watch\" \"nodemon --watch .build --delay 2.5 ./bin/www\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "migration:gen": "sequelize migration:create --migrations-path ./sequelize/migrations/ --name",
    "migration:run": "sequelize db:migrate --migrations-path sequelize/migrations"

  },
  "dependencies": {
    "@repo/backend": "workspace:*",
    "@types/uuid": "^11.0.0",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.1",
    "prettier": "^3.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/node": "^24.9.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@repo/types": "workspace:*",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.3",
    "typescript-json-schema": "^0.65.1"
  }
}
